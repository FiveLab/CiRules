<?xml version="1.0"?>
<ruleset name="Code standard for test sources">
    <rule ref="./phpcs.xml">
        <exclude name="Symfony.Commenting.ClassComment.Missing"/>
        <exclude name="Symfony.Commenting.Annotations"/>
        <exclude name="Symfony.Functions.ScopeOrder"/>
        <exclude name="Symfony.NamingConventions.ValidClassName"/>
        <exclude name="FiveLab.Commenting.Comment.MissingFunctionComment"/>
    </rule>
</ruleset>
